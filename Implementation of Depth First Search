#include<stdio.h>
#include<stdlib.h>


#define MAX 100

struct node {
int vertex;
struct node* next;
};

typedef struct node* GNODE;

GNODE graph[MAX];

int visited[MAX];

void DFS(int v) {
	visited[v] = 1;
	printf("\n");
	printf("%d", v);

	GNODE temp = graph[v];
	while(temp != NULL) {
		int adj = temp->vertex;
		if(!visited[adj]){
			DFS(adj);
		}
		temp = temp->next;
	}
}





}
